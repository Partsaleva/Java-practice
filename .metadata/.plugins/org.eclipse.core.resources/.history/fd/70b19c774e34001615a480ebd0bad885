
public class Reverse {

	public static void main(String[] args) {
		reverseIter("Hello");

	}

	private static String reverseIter(String str){
		char temp;
	    char[] arr = new char[str.length()];
	    arr = str.toCharArray();
	    int len = arr.length;
	    for(int i=0;i<(str.length())/2;i++){
	        temp = arr[i];
	        arr[i]=arr[len-1];
	        arr[len-1]=temp;
	        len--;
	    }
		return String.valueOf(arr);
		
	}
}
