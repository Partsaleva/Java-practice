
public class Test {

	public static void main(String[] args) {
		getIntegerComplement(50);

	}

	static int getIntegerComplement(int n) {
		Integer.toString(n, 2);
		String bin=Integer.toString(n, 2);
		Integer result=bin & 1;
		System.out.println(bin);
		System.out.println(~bin);
		
		return n;


    }

	int reverseBits(String binaryNum){
		
		char[] num=new char[binaryNum.length()];
		num = binaryNum.toCharArray();
		for (int i = 0; i < num.length; i++) {
			int j= Integer.parseUnsignedInt(num[i]);
			num[i]=Integer.t;
		}
		return 0;
		
	}
}
