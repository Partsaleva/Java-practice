import java.util.ArrayList;
import java.util.List;

public class Problem05 {

	public static void main(String[] args) {
		List<Integer> oddNums=oddNums();
		boolean found=false;
		boolean div=true;
		long number=20;
		
		while(!found){
			number++;
			if (number% 20 == 0) {
				for (Integer i : oddNums) {
					if (number % i != 0) {
						div=false;
					}
				}
				if (div) {
					found=true;
					System.out.println(number);
				}
			}
			
		}
	}

	private static List<Integer> oddNums(){
		List<Integer> nums=new ArrayList<>();
		for (int i = 3; i < 20; i=i+2) {
			nums.add(i);
		}
		return nums;
	}
}
