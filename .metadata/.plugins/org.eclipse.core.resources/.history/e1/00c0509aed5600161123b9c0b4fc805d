
public class Problem57 {

    public static void main(String[] args) {
        int count = 0;
         
        long den = 2;
        long num = 3;
         
        for (int i = 1; i < 1000; i++) {
             num += 2 * den;
             den = num - den;
             if (Math.log10(num) > Math.log10(den))
                 count++;
        }

        System.out.println(count);
    }

}
