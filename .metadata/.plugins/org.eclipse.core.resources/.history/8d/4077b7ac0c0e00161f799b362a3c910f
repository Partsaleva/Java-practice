package task3and4;

public class CalculateExpression {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	public static double resultOfFirstFormula(int a, int b, int c){
		int aSqrt = a * a;
		int bSqrt = b * b;
		double mainExpr = (aSqrt+bSqrt)/(aSqrt-bSqrt);
		double powExpr=(a+b+c)/Math.sqrt(c);
		return Math.pow(mainExpr, powExpr);
	}
	
	
	public static double resultOfSecondFormula(int a, int b, int c){
		int cCub=c*c*c;
		double mainExpr=a*a + b*b - cCub;
		double powExpr=a-b;
		return Math.pow(mainExpr, powExpr);
	}
}
