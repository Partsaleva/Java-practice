package h5_Regular_Expressions;

public class Problem11SemanticHTML {

    public static void main(String[] args) {
        String html1 = "<div style=\"color:red\" id=\"header\">\n"
                    + "</div> <!-- header -->\n" 
                    + "END";
        String html2 = "<div align=\"left\" id=\"nav\" style=\"color:blue\">\n" 
                    +  "<ul class=\"header\">\n"
                    + "<li id=\"main\">\n" 
                    + " Hi, I have id=\"main\".\n" 
                    + "</li>" + " </ul>\n"
                    + "</div> <!-- nav -->\n" 
                    + "END";

    }

}
