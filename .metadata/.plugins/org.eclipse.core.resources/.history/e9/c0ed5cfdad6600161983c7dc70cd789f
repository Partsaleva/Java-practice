
public class Test {

    public static void main(String[] args) {
      int number=7;
      for (int i = 0; i < number; i++) {
          for (int j = i; j < number-1; j++) {
              System.out.print(" ");
          }
          for (int j = i; j < number; j++) {
              System.out.print("#");
          }
          System.out.println();
      }

    }

}
