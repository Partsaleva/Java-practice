import java.util.Random;
import java.util.Scanner;

public class GuessTheNumber {

	public static void main(String[] args) {
		GuessTheNumber num=new GuessTheNumber();
		Scanner sc = new Scanner(System.in);
		int input = sc.nextInt();
		num.guessNumber(input);
	}

	public void guessNumber(int input){
		Random rand=new Random();
		int num=rand.nextInt(1001);	
		int begin=1,end=1000;
		while(begin <= end){
			int middle=(begin + end) / 2;
			int val=guess(input,num);
			if(val == 0){
				System.out.println("Bingo");
			}
			if(val<0){
				end=middle-1;
				val=guess(input,num);
			}
			else{
				begin=middle+1;
				val=guess(input,num);
			}
			
		}
	}

	private int guess(int input, int num) {
		if(input==num){
			return 0;
		}
		if (input<num) {
			return -1;
		} else {
			return 1;
		}
	}
	
	
	

}
