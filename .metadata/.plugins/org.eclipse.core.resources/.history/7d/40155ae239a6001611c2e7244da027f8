package h5_Regular_Expressions;

public class Problem11SemanticHTML {

    public static void main(String[] args) {
        String html1="<div style=\"color:red\" id=\"header\">"
                + "</div> <!-- header -->"
                + "END";
        String html2="<div align=\"left\" id=\"nav\" style=\"color:blue\">"
                + "<ul class=\"header\">"
                + "<li id=\"main\">"
                        +" Hi, I have id=\"main\"."
                                + "</li>"
                                + " </ul>"
                                + "</div> <!-- nav -->"
                                + "END";

    }

}
