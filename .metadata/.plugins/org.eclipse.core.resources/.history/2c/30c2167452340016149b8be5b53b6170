
public class Test {

	public static void main(String[] args) {
		System.out.println();

	}

	static void StairCase(int n) {
        int length=n;
        for(int i= 0; i< n; i++){
            for(int j= 0; j< length-i; j++){
            	System.out.print(' ');
            }
            
            System.out.print('#');
        }

    }
}
