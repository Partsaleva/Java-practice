import java.util.Random;
import java.util.Scanner;

public class GuessTheNumber {

	public static void main(String[] args) {
		GuessTheNumber num=new GuessTheNumber();
		Scanner sc = new Scanner(System.in);
		int input = sc.nextInt();
		num.guessNumber(input);
	}

	public void guessNumber(int input){
		Random rand=new Random();
		int num=rand.nextInt(1001);
		int left = 1, right = 1000;
		while (left <= right) {
		    int mid = (left + right) / 2;
		    int res = guess(input,num);
		    if (res == 0)
		        System.out.println("Bingo");
		    if (res < 0)
		        right = mid - 1;
		    else
		        left = mid + 1;
		}
	}
	
	
	public void guess(int num){
		int left = 1, right = 1000;
		while (left <= right) {
		    int mid = (left + right) / 2;
		    int res = guess(mid,num);
		    if (res == 0)
		        System.out.println("Bingo");
		    if (res < 0)
		        right = mid - 1;
		    else
		        left = mid + 1;
		}
		
	}
	
	public int guess(int x,int num){
		if (x==num) {
			return 0;
		} 
		if(x>num){
			return 1;
		}
		else {
			return -1;
		}
	}
}
