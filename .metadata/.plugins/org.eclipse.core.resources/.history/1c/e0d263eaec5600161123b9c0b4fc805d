
public class Problem57 {

    public static void main(String[] args) {
        int result = 0;
         
        long den = 2;
        long num = 3;
         
        for (int i = 1; i < 1000; i++) {
             num += 2 * den;
             den = num - den;
             if ((int)Math.log10(num) >(int)Math.log10(den))
                 result++;
        }

        System.out.println(result);
    }

}
