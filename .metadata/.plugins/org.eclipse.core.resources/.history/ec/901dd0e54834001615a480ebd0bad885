import java.util.ArrayList;
import java.util.List;

public class Problem05 {

	public static void main(String[] args) {
		List<Integer> primes=generatePrimes();
		boolean found=false;
		boolean div=true;
		long number=20;
		
		while(!found){
			number++;
			if (number% 20 == 0) {
				for (Integer i : primes) {
					if (number % i != 0) {
						div=false;
					}
				}
				if (div) {
					found=true;
					System.out.println(number);
				}
			}
			
		}
	}

	private static List<Integer> generatePrimes(int limit){
		List<Integer> nums=new ArrayList<>();
		boolean isPrime;
		int sqrtNum=0;
		for (int i = 3; i < limit; i=i+2) {
			isPrime=true;
			sqrtNum=(int) Math.sqrt(i);
			for (int j = 0; j < sqrtNum; j++) {
				if (i % nums.get(j) == 0) {
					isPrime=false;
					break;
				}
			}
			if (isPrime) {
				nums.add(i);
			}
		}
		return nums;
	}
}
