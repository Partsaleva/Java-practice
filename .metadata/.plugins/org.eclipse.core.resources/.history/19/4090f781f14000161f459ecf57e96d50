//Problem 2
public class SumCards {

    public static void main(String[] args) {
        String input="2C 2H 2D AS 10H 10C 2S KD";
        sumOfCards(input);
        input="AS KH 10C";
        sumOfCards(input);
        input ="2S 2C 2D 2H";
        sumOfCards(input);
        input ="AS 10C KS KH KD 9H JH QS 3H QD QH 8S 10D 10S 7C JD";
        sumOfCards(input);

    }

    public static int sumOfCards(String input){
        int sum=0;
        String suits="[SHDC]";
        input=input.replaceAll(suits, "");
        System.out.println(input);
        return sum;
    }
    
    private int valueOfNotNumCard(char card){
        int value=0;
        switch(card){
            case 'J': value= 12;
            break;
            case 'Q': value= 13;
            break;
            case 'K': value= 14;
            break;
            case 'A': value= 15;
            break;
        }
        return value;
    }
}
