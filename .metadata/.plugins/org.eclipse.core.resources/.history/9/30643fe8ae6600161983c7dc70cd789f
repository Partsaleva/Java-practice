
public class Test {

    public static void main(String[] args) {
      int number=7;
      for (int i = 1; i <number; i++) {
          for (int j = i; j <=number-1; j++) {
              System.out.print(" ");
          }
          for (int j = 1; j<=i; j++) {
              System.out.print("#");
          }
          System.out.println();
      }

    }

}
