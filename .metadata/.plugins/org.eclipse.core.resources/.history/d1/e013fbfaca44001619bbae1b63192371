
public class Problem3SimpleExpression {

    public static void main(String[] args) {
        String input="5 -33   + 12 -  55-  1  - 2+6";
        System.out.println(solve(input));
        input="1.5 + 2.5";
        System.out.println(solve(input));
        input="0.05+0.01 - 1";
        System.out.println(solve(input));
        input="1         +        2";
        System.out.println(solve(input));
        input="9876543210 + 0.987654321";
        System.out.println(solve(input));

    }

    public static double solve(String input){
        input= input.replaceAll("\\s+", "");
        double sum=0, m=0;
        String[] nums=input.split("\\+");
        for (String s : nums) {
            String[] min=s.split("-");
            for (String n : min) {
                m-=Double.parseDouble(n);
            }
            System.out.println(m);
            sum+=m;
        }
        return sum;
        
    }
}
