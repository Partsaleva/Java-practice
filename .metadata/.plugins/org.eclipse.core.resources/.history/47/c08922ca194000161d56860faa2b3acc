import java.util.ArrayList;
import java.util.List;

public class Problem1StuckNumbers {

    public static void main(String[] args) {
        int count=5;
        String input="2 51 1 75 25";
        printStuckNumbers(count, input);
        count=7;
        input="2 22 23 32 322 222 5";
        
        count=3;
        input="5 1 20";

    }

    public static void printStuckNumbers(int count, String input){
        String[] numbers=input.split(" ");
        List<String> results=new ArrayList<>();
        String a,b;
        for (int i = 0; i < numbers.length; i++) {
            for (int j = 0; j < numbers.length; j++) {
                if(numbers[i]==numbers[j]){
                    continue;
                }
                results.add(numbers[i] +"|"+ numbers[j]);
            }
        }
    }
}
